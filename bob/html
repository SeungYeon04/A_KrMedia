 -moz-filter: grayscale(100%);
}

.userEL11924077>.container>.grid-container>.grid-item>div>img:hover {
 filter: grayscale(0%);
 -webkit-filter: grayscale(0%);
 -moz-filter: grayscale(0%);
 transition: all 0.7s ease;
}
