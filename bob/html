<script type="text/javascript">
	var script_preloading = loadingScript({'forum':true});

	var LANG = getLanguage();
	setLanguage(LANG);
</script>



<script>
var windowOpen;
var memberCloseCheck = true;
var user = {
	'name' : '',
	'pass' : '',
	'key' : ''
};

var commentUser = {
	'id' : '',
	'reply' : '',
	'name' : '',
	'pass' : '',
	'parent' : '',
	'content' : '',
	'key' : '',
	'secret' : ''
}

var postUser = {
	'name' : '',
	'img' : '',
	'option' : '',
	'secret_display' : '',
	'replied' : '',
	'writeable'	: false,
	'sid' : false
};

var sid = '1309118',
	pid = '11911915',
	id = '',
	reply = '',
	PAGE_MODE = 'A';

var data = [];
var referer = '';

if (localStorage.referer) {
	referer = localStorage.referer;
}

clLocalStorageClear();
	localStorage.sid = sid;
	localStorage.type = 'P';
	localStorage.pid = '11911915';
	localStorage.view = '425318';
	localStorage.page_mode = 'A';
	localStorage.status = 'comment';
	localStorage.parent = '';
	localStorage.reply_name = '';
	localStorage.reply = '';
	localStorage.id = '';
	localStorage.content = '댓글';
	localStorage.secret = '';
	localStorage.href = '/Designer/view/425318';
	localStorage.forum_email = 'undefined';


$(function () {

		data['captcha'] = '1';
	var captcha = $.umember.makeJoinConditionsStr(sid,data,'captcha');
	load_kcaptcha();
	$("#captcha").html(captcha);

	$('#wr_key').on({
		blur: function (e) {
			var val = $(this).val().trim(),
				captcha_error = $(this).next('label.error'),
				error_class = '';
				error_str = '';
			
			if(val.length == 0) {
				error_class = 'empty';
				error_str = $.lang[LANG]['manager.join.regexp.captcha'];
			} else if( val && (Base64.encode(val) != md5_norobot_key)) {
				error_class = 'error';
				error_str = $.lang[LANG]['manager.join.regexp.captcha.wrong'];
			}

			if(error_str.length > 0) {
				$(this).css('border-color','#ee445f');
				$(this).closest('#captcha').addClass(error_class);
			} else {
				$(this).removeAttr('style');
				$(this).closest('#captcha').removeClass('empty').removeClass('error');
			}
		}
	});
	


	$(document).on('click', '.cl-s-login-switch2 li', function () {
		var login_type = $(this).attr('data-mode');
		if($(this).hasClass('active')) return false;
		$(this).fadeOut(0).addClass('active').siblings().removeClass('active').fadeIn(0);
		clearLogininput();

		if (login_type == 'mem') {
			$('.cl-s-title').html('관리자');
			$('.cl-s-login-box[data-mode="mem"]').fadeIn(0);
			$('.cl-s-login-box[data-mode="nonmem"]').fadeOut(0);
		} else {
			$('.cl-s-title').html('비회원');
			$('.cl-s-login-box[data-mode="mem"]').fadeOut(0);
			$('.cl-s-login-box[data-mode="nonmem"]').fadeIn(0);
		}
	});

	$(document).on('click', '.cl-s-login-switch li', function () {
		var login_type = $(this).attr('data-mode');
		if($(this).hasClass('active')) return false;
		$(this).addClass('active').siblings().removeClass('active');
		clearLogininput();
		if (login_type == 'mem') {
			$('.cl-s-login-box[data-mode="mem"]').fadeIn(0);
			$('.cl-s-login-box[data-mode="nonmem"]').fadeOut(0);
		} else {
			$('.cl-s-login-box[data-mode="mem"]').fadeOut(0);
			$('.cl-s-login-box[data-mode="nonmem"]').fadeIn(0);
		}
	});


	$('.cl-s-form-group').on('click',function(e) {
		if($(e.target).hasClass('cl-s-form-group')) {
			$(e.target).find('.cl-s-form-control:not([type=hidden])').focus();
		}
	});

	$('.cl-s-form-control').on({
		keyup: function(e) {
			if(e.keyCode === 13) {
				if($(this).closest('.cl-s-form-wrap').next('.cl-s-form-wrap').length > 0) {
					$(this).closest('.cl-s-form-wrap').next('.cl-s-form-wrap').find('.cl-s-form-control').focus();
				} else if($(this).closest('.cl-s-form-wrap').next('#captcha:not(:empty)').length > 0) {
					$('#wr_key').focus();
				} else {
					var loginBoxMode = $(this).closest('.cl-s-login-box').attr('data-mode');
					if(loginBoxMode == 'mem') $('#login-btn').click();
					else $('.login-nonemem-btn').click();
				}
			}
		},
		focus: function(e) {
			$(this).closest('.cl-s-form-wrap').addClass('active');
		},
		blur: function(e) {
			var inputEL = $(this),
				type = $(this).attr('name'),
				regexp = {
					id: /^[A-Za-z0-9+]{4,20}$/i,
					email : /^[a-zA-Z0-9+-\_.]+@[a-zA-Z0-9-]+\.[a-zA-Z0-9-.]+$/i,
					nonmem_id : /^[a-zA-Z0-9가-힣]*$/i,
				},
				val = $(this).val().trim();

			inputEL.closest('.cl-s-form-wrap').removeClass('active');
			if(val.length == 0) {
				inputEL.closest('.cl-s-form-wrap').removeClass('error').addClass('empty');
				inputEL.closest('.cl-s-form-wrap').find('.cl-s-control-label.error').remove();
				return false;
			} else {
				inputEL.closest('.cl-s-form-wrap').removeClass('empty');
				var error_str = '';
				if(checkEmojis(val)) {
					error_str = $.lang[LANG]['config.unable.emoji'];
				} else if(type == 'id' && val != 'admin') {
					if(!regexp['email'].test(val)) error_str = $.lang[LANG]['siteum.regexp.check.id'];
				} else if(type == 'password') {
					if(val.length<4) error_str = $.lang[LANG]['config.pass-min-length'];
				} else if(type == 'nonmem_id') {
					if(!regexp['nonmem_id'].test(val) || val.length < 2 || val.length > 20) {
						error_str = '이름은 한글, 알파벳, 숫자로 20자까지만 사용 가능합니다.';
					} else if(val == 'admin') {
						error_str = $.lang[LANG]['config.id-admin-check'];
					}
				}

				if(error_str.length > 0) {
					inputEL.closest('.cl-s-form-wrap').addClass('error');
					if(inputEL.parent().find('.cl-s-control-label.error').length > 0) {
						inputEL.parent().find('.cl-s-control-label.error').html(error_str);
					} else {
						inputEL.after('<label for="input" class="cl-s-control-label error">'+error_str+'</label>');
					}
					return false;
				}
			}

			inputEL.closest('form').find('.cl-s-result-wrap.error').removeClass('error').empty();
			inputEL.closest('.cl-s-form-wrap').removeClass('error').removeClass('empty');
			inputEL.closest('.cl-s-form-wrap').find('.cl-s-control-label.error').remove();
		}
	});


	$(document).on('click', '.login-btn', function() {
		var loginBoxMode = $(this).closest('.cl-s-login-box').attr('data-mode'),
			activeLoginBox = $('form.cl-s-login-box[data-mode="' + loginBoxMode + '"]'),
			checkNonmemLogin = ($(this).hasClass('n-login-btn')) ? true : false,
			checkWriteLogin = ($(this).hasClass('write-login-btn')) ? true : false,
			checkCaptcha = (activeLoginBox.find('#captcha:not(:empty)').find('#wr_key').length > 0) ? true : false;

		if($('input[type="hidden"][name="sns_key"]').val().trim().length == 0) {
			activeLoginBox.find('.cl-s-form-wrap').each(function(e) {
				var check_input = $(this).find('.cl-s-form-control'),
					val = check_input.val().trim();

				if(val.length == 0) {
					$(this).removeClass('error').addClass('empty');
					$(this).find('.cl-s-control-label.error').remove();
				} else {
					if($(this).hasClass('error')) {
						check_input.focus();
					}
				}
			});
		}

		if(checkCaptcha) {
			var wrKey = activeLoginBox.find('#captcha #wr_key');
			var captchaVal = wrKey.val();
			var error_str = '';
			
			wrKey.removeClass('empty error');
			wrKey.removeAttr('style');
			wrKey.next('label.error').remove();

			if(captchaVal.length == 0) {
				wrKey.addClass('empty');
				error_str = $.lang[LANG]['manager.join.regexp.captcha'];
			} else if( captchaVal && (Base64.encode(captchaVal) != md5_norobot_key)) {
				wrKey.addClass('error');
				error_str = $.lang[LANG]['manager.join.regexp.captcha.wrong'];
			}

			if (wrKey.hasClass('empty') || wrKey.hasClass('error')) {
				wrKey.after('<label class="error">' + error_str + '</label>');
				wrKey.css('border-color','#ee445f');
				wrKey.focus();

				return false;
			}
		}

		if(activeLoginBox.find('.cl-s-form-wrap.empty, .cl-s-form-wrap.error').length > 0) {
			activeLoginBox.find('.cl-s-form-wrap.empty, .cl-s-form-wrap.error').eq(0).find('.cl-s-form-control').focus();
			return false;
		}
		if(activeLoginBox.find('.cl-s-result-wrap.error').length > 0) return false;

		if(checkNonmemLogin) {			/*Nonmem - login proc*/
			// console.log('Nonmem  - login proc');
			var f = document.n_login_form;
			var status = 'comment';

			if (status == 'comment') {
				nameLength = getBytes( f.nonmem_id.value );
												commentUser.name = data.name = f.nonmem_id.value;
				commentUser.nameLength = nameLength;
				commentUser.pass = hex_md5( f.nonmem_password.value );
				commentUser.key = (checkCaptcha) ? activeLoginBox.find('#captcha:not(:empty)').find('#wr_key').val() : '';
				commentUser.content = '댓글';
				commentUser.secret = '';

				loginPostComment(commentUser, function(data) {
					location.href="/Designer/view/425318";
				});

			} else {

				var is_new = (id != '') ? 'false' : 'true';
				$.post('/fm/checkUser', { pid : pid, reply : reply, id : id, isNew : is_new }, function(data) {
					if(!data.user || typeof data.user == 'undefined') {
						if(typeof data.error != 'undefined' && data.error) {
							$(this).showModalFlat('ERROR',data.error,true,false,'','ok');
							return false;
						}

						resetObject(user);
						$('.modal[id*=flat-modal]').modal('hide');
						nameLength = getBytes( f.nonmem_id.value );
						user.name = data.name = f.nonmem_id.value;
						user.nameLength = nameLength;
						user.pass = hex_md5( f.nonmem_password.value );
						user.key = (checkCaptcha) ? activeLoginBox.find('#captcha:not(:empty)').find('#wr_key').val() : '';

						loginSetWriteUser(data,function() {
							location.href = '/Designer/view/425318';
						});
					} else {
						loginSetWriteUser(data,function() {
							location.href = '/Designer/view/425318';
						});
					}
				},'json');

			}


		} else if(checkWriteLogin) {	/*Write	 - login proc*/
			console.log('Write  - login proc');
			var f = document.login_form;
			var status = 'comment';

			$.post('/member/login/cllogin', { sid : sid, mb_id : f.id.value.toLowerCase(), mb_password : f.password.value }, function(data) {
				if(typeof data.error != 'undefined' && data.error) {
					if(typeof data.error == 'string') {
						activeLoginBox.find('.cl-s-form-control').blur();
						activeLoginBox.find('.cl-s-result-wrap').addClass('error').html(data.error);
					} else {
						$.each(data.error, function(k,error) {
							var errorInput = activeLoginBox.find('#mem_'+k);

							errorInput.closest('.cl-s-form-wrap').addClass('error');
							if(errorInput.parent().find('.cl-s-control-label.error').length > 0) {
								errorInput.parent().find('.cl-s-control-label.error').html(error);
							} else {
								errorInput.after('<label for="input" class="cl-s-control-label error">'+error+'</label>');
							}
						});
					}
					return false;
				}

				setTimeout(function () {
					if (status == 'comment') {
						$.post('/template/checkLogin', { comment : true }, function(ch_data) {
							commentUser.content = '댓글';
							commentUser.secret = '';
																					loginPostComment(commentUser, function(c_data) {
								location.href="/Designer/view/425318";
							});
						});
					} else {
						$.post('/fm/checkUser', { pid : pid, reply : reply, id : id }, function(sm_data) {
							var smData = JSON.parse(sm_data);
							loginSetWriteUser(smData,function() {
								location.href = '/Designer/view/425318';
							});
						});
					}
				}, 300);

			}, 'json');


		} else { 						/*Default  - login proc*/
			console.log('Default  - login proc');
			var status = 'comment';
			if(status == 'login' || status == 'like' || status == 'forum_view') {
				console.log('cllogin');
				var f = document.login_form;
				var status = 'comment';

				$.post('/member/login/cllogin', { sid : sid, mb_id : f.id.value.toLowerCase(), mb_password : f.password.value }, function(data) {
					if(typeof data.error != 'undefined' && data.error) {
						if(typeof data.error == 'string') {
							activeLoginBox.find('.cl-s-form-control').blur();
							activeLoginBox.find('.cl-s-result-wrap').addClass('error').html(data.error);
						} else {
							$.each(data.error, function(k,error) {
								var errorInput = activeLoginBox.find('#mem_'+k);

								errorInput.closest('.cl-s-form-wrap').addClass('error');
								if(errorInput.parent().find('.cl-s-control-label.error').length > 0) {
									errorInput.parent().find('.cl-s-control-label.error').html(error);
								} else {
									errorInput.after('<label for="input" class="cl-s-control-label error">'+error+'</label>');
								}
							});
						}
						return false;
					}
					location.href = '/Designer/view/425318';
				}, 'json');
				return false;
			}
			
			var f = document.login_form;
			var input_data = {
				'sns_key' : f.sns_key.value,
				'id' : f.id.value.toLowerCase(),
				'password' : hex_md5(f.password.value),
			};
			$('.login-btn').addClass('spinner');

			var rf = 'https://kumamid.com/Designer/view/425318';
			$.post('/umember/login/in', { sid : sid, data : JSON.stringify(input_data), mode : f.mode.value }, function(data) {

					if (data.result == 'fail') {
						$('.login-btn').removeClass('spinner');
						alert(data.message);
						return false;
					} else {

						if(typeof data.result != 'undefined' && data.result == 'success') {
							if(typeof data.ss_url != 'undefined' && data.ss_url) {
								$.each(data.ss_url, function(i,url) {
									$('body').append('<img class="auth-img" src="' + url + '">');
								});
							}

							if(typeof data.ga_uid != 'undefined' && data.ga_uid) {
								localStorage.setItem('googleAnalyticsUID'+sid, data.ga_uid);
							}
						}

						setTimeout(function () {
							if (typeof data.goto_url != 'undefined' && data.goto_url) {
								location.href = data.goto_url;
							} else {
								if (referer) location.href = referer;
								else location.href = (rf) ? rf : '/';
							}
						}, 1300);

					}
			}, 'json');
		}
	});

	// $(document).on('click', '.btn.btn-primary.ok-button-dialog.btn-sm.close-button-dialog', function () {
	// 	history.go(-1);
	// });
});


function resetObject(o) {
	$.each(o,function(k,v) {
		o[k] = '';
	});
	return o;
}

function loginSetWriteUser (data, callback) {
	if(data.writeable == false) {
		var access = data.access;
		var message = 'error';
		
		switch (access) {
			case 'G':
				message = $.lang[LANG]['board.group-can-write'];
				break;
			case 'ADM':
				message = $.lang[LANG]['board.admin-can-write']
				break;
			default:
				break;
		}

		$(this).showModalFlat('INFORMATION', message,true,false,'','ok','','cl-cmmodal cl-s-btn w560 cl-p130 cl-t80 cl-modal cl-none-title', false, '', '', function() {
			location.href = '/';
		});

		return false;
	}

	resetObject(postUser);
	postUser.name = data.name;
	postUser.img = data.myimg;
	postUser.option = data.option;
	postUser.secret_display = (data.settings !== undefined && data.settings)? data.settings.secret_display : '';
	postUser.mgr = data.mgr;
	postUser.replied = data.replied;
	postUser.writeable = data.writeable;
	$.post('/fm/guest/login', { user : postUser, guest : user }, function(data) {
		if(typeof callback == 'function') {
			callback();
		}
	});
	return false;
}

var loginPostComment = function(commentUser, callback) {
	var cSID = (PAGE_MODE == 'c') ? SID : property.SID,
		cTYPE = 'P',
		cPID = '11911915',
		cVIEW = '425318',
		forum_email = (PAGE_MODE == 's') ? 'undefined' : '';

	$.post('/template/comment/update', { user : JSON.stringify(commentUser), type : cTYPE, pid : cPID, view : cVIEW, sid : cSID, forum_email : forum_email }, function(r) {
		if(typeof r.error != 'undefined' && r.error) {
			$(this).showModalFlat('ERROR',r.error,true,false,'','ok');
			return false;
		}

		// $.comment.init();

		if(typeof callback === 'function') {
			callback(r);
		}
	},'json');
}




function clearLogininput() {
	$('.cl-s-form-wrap').each(function(e) {
		$(this).removeClass('active').removeClass('empty').removeClass('error');
		$(this).find('.error').remove();
		$(this).find('.cl-s-form-control').val('');
	});

	$('.cl-s-result-wrap.error').removeClass('error').empty();
	$('#wr_key').val('').removeAttr('style');
}

</script>

